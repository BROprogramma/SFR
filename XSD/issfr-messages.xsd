                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="CorrectionReasonType">
      <xs:simpleContent>
         <xs:restriction base="gml:CodeWithAuthorityType">
            <xs:attribute name="codeSpace"
                          type="xs:anyURI"
                          use="required"
                          fixed="urn:bro:bhrgt:CorrectionReason"/>
         </xs:restriction>
      </xs:simpleContent>
   </xs:complexType>
   <xs:element name="SourceDocument" type="issfr:SourceDocumentType">
      <xs:annotation>
         <xs:documentation source="http://www.imvertor.org/model-info/technical-documentation">Definition: Deelonderzoek van een wandonderzoek booronderzoek. Dit is of een wandbeschrijving of eenwandmonsteranalyse (fase 1). Later zal de wandmonsteranalyse worden uitgebreid (fase 2). Later zal de reeks van deelonderzoeken worden uitgebreid metfoto's.</xs:documentation>
      </xs:annotation>
   </xs:element>
   <xs:complexType name="SourceDocumentType">
      <xs:choice minOccurs="1" maxOccurs="1">
         <xs:element name="SFR_CompleteReport_V1" type="issfr:SFR_CompleteReport_V1Type"/>
         <xs:element name="SFR_StartReport_V1" type="issfr:SFR_StartReport_V1Type"/>
         <xs:element name="SFR_EndReport_V1" type="issfr:SFR_EndReport_V1Type"/>
      </xs:choice>
   </xs:complexType>
   <xs:complexType name="ResearchAreaType">
      <xs:choice minOccurs="1" maxOccurs="1">
         <xs:element ref="sfrcom:SoilFaceSampleAnalysis"/>
         <xs:element ref="sfrcom:SoilFaceDescription"/>
      </xs:choice>
   </xs:complexType>
</xs:schema>
