<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:issfr="http://www.broservices.nl/xsd/issfr/2.0"
           xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:imvert="http://www.imvertor.org/schema/system"
